{
  "name": "meanm",
  "version": "1.0.0",
  "description": "fullstack app with Node and Angular",
  "main": "index.js",
  "engines": {
    "node": "18.12.1",
    "npm": "8.19.2"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build":  "npm install && cd ./client && npm i && npm run build"
  },
  "keywords": [
    "angular"
  ],
  "author": "Stanislav Kadyrov",
  "license": "ISC",
  "dependencies": {
    "@angular/cli": "^14.2.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^6.7.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20"
  }
}
